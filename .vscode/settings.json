{
    "cSpell.words": [
      "canonicalization",
      "conda",
      "Marximus",
      "Maximus",
      "Referentials",
      "rreadlink"
    ],
    "files.eol": "\n",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "better-comments.highlightPlainText": true,
    "python.defaultInterpreterPath": "/opt/conda/miniforge/bin/python",
    "python.disableInstallationCheck": true,
    "python.testing.unittestEnabled": true,
    "python.testing.pytestEnabled": true,
    "shell-syntax.defaultShell": "sh",
    "shellcheck.executablePath": "/opt/conda/miniforge/envs/shellcheck/bin/shellcheck",
    "editor.showFoldingControls": "always",
    "editor.rulers": [80,120],
    "better-comments.tags": [
        {
          "tag": "NOTE:",
          "color": "#98C379",
          "strikethrough": false,
          "underline": false,
          "backgroundColor": "transparent",
          "bold": false,
          "italic": false
        },
        {
            "tag": "WARNING:",
            "color": "#FF2D00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": true,
            "italic": false
        },
        {
            "tag": "HACK:",
            "color": "#FF2D00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
          "tag": "QUESTION:",
          "color": "#3498DB",
          "strikethrough": false,
          "underline": false,
          "backgroundColor": "transparent",
          "bold": false,
          "italic": false
        },
        {
          "tag": "TODO:",
          "color": "#FF8C00",
          "strikethrough": false,
          "underline": false,
          "backgroundColor": "transparent",
          "bold": false,
          "italic": false
        },
        {
          "tag": "shellcheck",
          "color": "#666666",
          "strikethrough": false,
          "underline": false,
          "backgroundColor": "transparent",
          "bold": false,
          "italic": false
        }
      ]
}
